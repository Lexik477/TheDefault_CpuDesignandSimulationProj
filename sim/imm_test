#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x6396f79b9e90 .scope module, "test_imm" "test_imm" 2 4;
 .timescale -9 -12;
v0x6396f79cca40_0 .net "imm", 31 0, v0x6396f79814a0_0;  1 drivers
v0x6396f79ccb30_0 .var "imm_sel", 1 0;
v0x6396f79ccc00_0 .var "instruction", 31 0;
S_0x6396f79ba010 .scope module, "dut" "immediateGen" 2 9, 3 4 0, S_0x6396f79b9e90;
 .timescale -9 -12;
    .port_info 0 /INPUT 32 "instruction"
    .port_info 1 /INPUT 2 "imm_sel"
    .port_info 2 /OUTPUT 32 "imm"
v0x6396f79814a0_0 .var "imm", 31 0;
v0x6396f79cc820_0 .net "imm_sel", 1 0, v0x6396f79ccb30_0;  1 drivers
v0x6396f79cc900_0 .net "instruction", 31 0, v0x6396f79ccc00_0;  1 drivers
E_0x6396f79ba440 .event edge, v0x6396f79cc820_0, v0x6396f79cc900_0;
    .scope S_0x6396f79ba010;
T_0 ;
    %wait E_0x6396f79ba440;
    %load/vec4 v0x6396f79cc820_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6396f79814a0_0, 0, 32;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 1, 31, 6;
    %replicate 20;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 12, 20, 6;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x6396f79814a0_0, 0, 32;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 1, 31, 6;
    %replicate 20;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 7, 25, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 5, 7, 4;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x6396f79814a0_0, 0, 32;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 1, 31, 6;
    %replicate 20;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 1, 7, 4;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 6, 25, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 4, 8, 5;
    %concat/vec4; draw_concat_vec4
    %concati/vec4 0, 0, 1;
    %store/vec4 v0x6396f79814a0_0, 0, 32;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x6396f79cc900_0;
    %parti/s 20, 12, 5;
    %concati/vec4 0, 0, 12;
    %store/vec4 v0x6396f79814a0_0, 0, 32;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x6396f79b9e90;
T_1 ;
    %vpi_call 2 12 "$display", " Immediate Generator Test " {0 0 0};
    %pushi/vec4 4194451, 0, 32;
    %store/vec4 v0x6396f79ccc00_0, 0, 32;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x6396f79ccb30_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 17 "$display", "I-type: 0x%h -> 0x%h", v0x6396f79ccc00_0, v0x6396f79cca40_0 {0 0 0};
    %pushi/vec4 2129955, 0, 32;
    %store/vec4 v0x6396f79ccc00_0, 0, 32;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x6396f79ccb30_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 22 "$display", "S-type: 0x%h -> 0x%h", v0x6396f79ccc00_0, v0x6396f79cca40_0 {0 0 0};
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
