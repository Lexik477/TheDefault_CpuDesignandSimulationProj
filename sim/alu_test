#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x60cbb096c090 .scope module, "test_alu" "test_alu" 2 3;
 .timescale -9 -12;
v0x60cbb0980ac0_0 .var "a", 31 0;
v0x60cbb0980ba0_0 .var "alu_control", 3 0;
v0x60cbb0980c70_0 .var "b", 31 0;
v0x60cbb0980d70_0 .net "result", 31 0, v0x60cbb0980830_0;  1 drivers
v0x60cbb0980e40_0 .net "zero", 0 0, L_0x60cbb0990fa0;  1 drivers
S_0x60cbb096c210 .scope module, "dut" "alu" 2 9, 3 4 0, S_0x60cbb096c090;
 .timescale -9 -12;
    .port_info 0 /INPUT 32 "a"
    .port_info 1 /INPUT 32 "b"
    .port_info 2 /INPUT 4 "alu_control"
    .port_info 3 /OUTPUT 32 "result"
    .port_info 4 /OUTPUT 1 "zero"
P_0x60cbb0948c10 .param/l "ALU_ADD" 1 3 14, C4<0000>;
P_0x60cbb0948c50 .param/l "ALU_AND" 1 3 16, C4<0010>;
P_0x60cbb0948c90 .param/l "ALU_OR" 1 3 17, C4<0011>;
P_0x60cbb0948cd0 .param/l "ALU_SUB" 1 3 15, C4<0001>;
L_0x7c835e8a9018 .functor BUFT 1, C4<00000000000000000000000000000000>, C4<0>, C4<0>, C4<0>;
v0x60cbb0934650_0 .net/2u *"_s0", 31 0, L_0x7c835e8a9018;  1 drivers
v0x60cbb09805b0_0 .net "a", 31 0, v0x60cbb0980ac0_0;  1 drivers
v0x60cbb0980690_0 .net "alu_control", 3 0, v0x60cbb0980ba0_0;  1 drivers
v0x60cbb0980750_0 .net "b", 31 0, v0x60cbb0980c70_0;  1 drivers
v0x60cbb0980830_0 .var "result", 31 0;
v0x60cbb0980960_0 .net "zero", 0 0, L_0x60cbb0990fa0;  alias, 1 drivers
E_0x60cbb0934c50 .event edge, v0x60cbb0980690_0, v0x60cbb09805b0_0, v0x60cbb0980750_0;
L_0x60cbb0990fa0 .cmp/eq 32, v0x60cbb0980830_0, L_0x7c835e8a9018;
    .scope S_0x60cbb096c210;
T_0 ;
    %wait E_0x60cbb0934c50;
    %load/vec4 v0x60cbb0980690_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x60cbb0980830_0, 0, 32;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x60cbb09805b0_0;
    %load/vec4 v0x60cbb0980750_0;
    %add;
    %store/vec4 v0x60cbb0980830_0, 0, 32;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x60cbb09805b0_0;
    %load/vec4 v0x60cbb0980750_0;
    %sub;
    %store/vec4 v0x60cbb0980830_0, 0, 32;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x60cbb09805b0_0;
    %load/vec4 v0x60cbb0980750_0;
    %and;
    %store/vec4 v0x60cbb0980830_0, 0, 32;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x60cbb09805b0_0;
    %load/vec4 v0x60cbb0980750_0;
    %or;
    %store/vec4 v0x60cbb0980830_0, 0, 32;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x60cbb096c090;
T_1 ;
    %vpi_call 2 12 "$display", "=== ALU Unit Test ===" {0 0 0};
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x60cbb0980ac0_0, 0, 32;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x60cbb0980c70_0, 0, 32;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x60cbb0980ba0_0, 0, 4;
    %delay 10000, 0;
    %vpi_call 2 17 "$display", "ADD: %d + %d = %d", v0x60cbb0980ac0_0, v0x60cbb0980c70_0, v0x60cbb0980d70_0 {0 0 0};
    %pushi/vec4 61680, 0, 32;
    %store/vec4 v0x60cbb0980ac0_0, 0, 32;
    %pushi/vec4 65280, 0, 32;
    %store/vec4 v0x60cbb0980c70_0, 0, 32;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x60cbb0980ba0_0, 0, 4;
    %delay 10000, 0;
    %vpi_call 2 22 "$display", "AND: 0x%h & 0x%h = 0x%h", v0x60cbb0980ac0_0, v0x60cbb0980c70_0, v0x60cbb0980d70_0 {0 0 0};
    %vpi_call 2 24 "$display", "=== ALU Test Complete ===" {0 0 0};
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
